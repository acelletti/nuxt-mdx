{
  "name": "@acelletti/nuxt-mdx",
  "version": "0.0.2",
  "description": "Write JSX in your Markdown with Nuxt",
  "repository": "acelletti/nuxt-mdx",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "npm run dev:prepare",
    "prepack": "npm run prepack:build && npm run prepack:clean",
    "prepack:build": "nuxt-module-build build",
    "prepack:clean": "clean-package",
    "postpack": "clean-package restore",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@mdx-js/vue": "^3.0.1",
    "@nuxt/kit": "^3.11.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.1",
    "@nuxt/test-utils": "^3.12.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.29",
    "changelogen": "^0.5.5",
    "clean-package": "^2.2.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "nuxt": "^3.11.1",
    "sass": "^1.72.0",
    "vitest": "^1.4.0"
  },
  "clean-package": {
    "remove": [
      "scripts",
      "devDependencies",
      "clean-package"
    ]
  }
}
